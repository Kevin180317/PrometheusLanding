---
import Prices from "@Components/prices/Hero.astro";
import Product from "@Components/prices/Product";
import Layout from "@Layouts/Layout.astro";
import { getLangFromUrl, useTranslations } from "@src/i18n/utils";

const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
---

<Layout title="PrometheusTij - Prices">
  <Product
    client:load
    categoria="Prototipo"
    ProtDesc1={t("product.prot.desc1")}
    ProtDesc2={t("product.prot.desc2")}
  />
  <!-- <Prices /> -->
</Layout>
