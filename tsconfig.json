{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Layouts/*": [
        "src/layouts/*"
      ],
      "@Components/*": [
        "src/components/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@fonts/*": [
        "public/fonts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}